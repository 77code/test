{
    "name": "trinhdev/flarum-logo",
    "description": "Adds logo to website",
    "keywords": [
        "flarum",
        "flarum logo"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0.0"
    },
    "authors": [{
        "name": "Huynh Do Phi Trinh",
        "email": "info.trinhdev@gmail.com",
        "role": "Developer"
    }],
    "support": {
        "source": "https://github.com/77code/flarum-logo"
    },
    "autoload": {
        "psr-4": {
            "Trinnhdev\\Logo\\": "src/"
        }
    },
    "homepage": "https://trinhhuynh.tk",
    "extra": {
        "flarum-extension": {
            "title": "Logo",
            "category": "feature",
            "icon": {
                "name": "fas fa-birthday-cake",
                "color": "#fff",
                "backgroundColor": "#D3241C"
            }
        }
    }
}